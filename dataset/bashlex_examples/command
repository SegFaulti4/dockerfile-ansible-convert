        PipeNode(pipe='|', pos=(907, 908)),
        CommandNode(pos=(909, 986), parts=[
          WordNode(pos=(909, 912), word='awk'),
          WordNode(pos=(913, 986), word='system("[ -e /usr/local/lib/" $1 " ]") == 0 { next } { print "so:" $1 }'),
