    CommandsubstitutionNode(command=
      PipelineNode(pos=(1370, 1549), parts=[
        CommandNode(pos=(1370, 1436), parts=[
          WordNode(pos=(1370, 1377), word='scanelf'),
          WordNode(pos=(1378, 1386), word='--needed'),
