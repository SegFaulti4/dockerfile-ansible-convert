                    }
                }
            ],
            "tar zvxf <<file : p>> -C <<dir : p>>": [
                {
                    "unarchive": {
                        "src": "<<file>>",
                        "dest": "<<dir>>",
                        "remote_src": "yes"
                    }
                }
            ],
            "tar xvzf <<file : p>> -C <<dir : p>>": [
                {
                    "unarchive": {
                        "src": "<<file>>",
                        "dest": "<<dir>>",
                        "remote_src": "yes"
                    }
                }
            ]
        }
    }
}