                    }
                }
            ],
            "tar xzvf <<file : p>> -C <<dir : p>>": [
                {
                    "unarchive": {
                        "src": "<<file>>",
                        "dest": "<<dir>>",
                        "remote_src": "yes"
                    }
                }
            ],
            "tar zvxf <<file : p>>": [
                {
