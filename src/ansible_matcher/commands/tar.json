{
    "tar": {
        "entry": "tar << parameters : m >>",
        "opts": {
            "extract": {
                "arg_required": false,
                "many_args": false,
                "aliases": [
                    "-x",
                    "--extract",
                    "--get"
                ]
            },
            "file": {
                "arg_required": true,
                "many_args": false,
                "aliases": [
                    "-f",
                    "--file"
                ]
            },
            "directory": {
                "arg_required": true,
                "many_args": false,
                "aliases": [
                    "-C",
                    "--directory"
                ]
            },
            "gzip": {
                "arg_required": false,
                "many_args": false,
                "aliases": [
                    "-z",
                    "--gzip"
                ]
            },
            "bzip2": {
                "arg_required": false,
                "many_args": false,
                "aliases": [
                    "-j",
                    "--bzip"
                ]
            },
            "xz": {
                "arg_required": false,
                "many_args": false,
                "aliases": [
                    "-J",
                    "--xz"
                ]
            }
        },
        "opts_postprocess_map": {
            "-z": {},
            "-j": {},
            "-J": {}
        },
        "examples": {
            "tar -x -f << file : p >>": {
                "unarchive": {
                    "src": "<< file >>",
                    "dest": "<< cwd >>",
                    "remote_src": "yes"
                }
            },
            "tar -x -f << file : p >> -C << dir : p >>": {
                "unarchive": {
                    "src": "<< file >>",
                    "dest": "<< dir >>",
                    "remote_src": "yes"
                }
            }
        }
    }
}