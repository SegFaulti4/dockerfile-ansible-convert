{
    "chown": {
        "entry": "chown <<parameters : m>>",
        "opts": {
            "quiet": {
                "aliases": ["--quiet", "--silent", "-f"],
                "arg_required": false,
                "many_args": false
            },
            "recursive": {
                "aliases": ["--recursive", "-R"],
                "arg_required": false,
                "many_args": false
            },
            "no dereference": {
                "arg_required": false,
                "many_args": false,
                "aliases": ["-h", "--no-dereference"]
            },
            "verbose": {
                "aliases": [ "--verbose",  "-v"],
                "arg_required": false,
                "many_args": false
            }
        },
        "examples": {
            "chmod <<owner>>:<<group>> <<files : m>>": [
                {
                    "shell": "ls -1d <<files>>",
                    "register": "ls_reg"
                },
                {
                    "file": {
                        "path": "{{ item }}",
                        "owner": "<<owner>>",
                        "group": "<<group>>"
                    },
                    "loop": "{{ ls_reg.stdout_lines }}"
                }
            ],
            "chmod <<owner>> <<files : m>>": [
                {
                    "shell": "ls -1d <<files>>",
                    "register": "ls_reg"
                },
                {
                    "file": {
                        "path": "{{ item }}",
                        "owner": "<<owner>>"
                    },
                    "loop": "{{ ls_reg.stdout_lines }}"
                }
            ]
        },
        "opts_postprocess": {}
    }
}